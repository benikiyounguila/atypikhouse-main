{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "webpack --config webpack.client.js",
    "start": "webpack serve --config webpack.client.js --open",
    "dev": "vite",
    "preview": "vite preview",
    "build:ssr": "vite build --ssr src/entry-server.jsx --outDir dist-ssr",
    "build:client": "vite build",
    "test": "cross-env VITE_BASE_URL=http://localhost:4000 jest --config jest.config.cjs",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.7",
    "@radix-ui/react-dialog": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-oauth/google": "^0.11.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "axios": "^1.2.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "daisyui": "^4.12.23",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "http-proxy": "^1.18.1",
    "jwt-decode": "^3.1.2",
    "lucide-react": "^0.483.0",
    "next": "^15.1.7",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-day-picker": "^8.8.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-loader-spinner": "^5.3.4",
    "react-router-dom": "^6.29.0",
    "react-toastify": "^9.1.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@tailwindcss/postcss": "^4.0.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/express": "^5.0.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-import-meta": "^2.3.2",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
